{
  "name": "angular-unit-test-helper",
  "version": "1.4.1",
  "description": "Helper functions to help write unit tests in Angular using mocks and spies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo 'I need tests'",
    "test:karma": "karma start",
    "dev": "webpack-dev-server --inline --hot",
    "build:webpack": "webpack -p",
    "standardize": "import-sort --write \"{lib,tests}/**/*.ts\" && prettier-tslint fix \"{lib,tests}/**/*.ts\"",
    "standardize:ci": "import-sort -l \"{lib,tests}/**/*.ts\" && prettier-tslint check \"{lib,tests}/**/*.ts\"",
    "build": "tsc",
    "pretest": "npm run build",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git push origin --all; git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/duluca/angular-unit-test-helper.git"
  },
  "keywords": [
    "angular",
    "unit",
    "test",
    "jasmine",
    "mock",
    "spy"
  ],
  "author": "Doguhan Uluca",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duluca/angular-unit-test-helper/issues"
  },
  "homepage": "https://github.com/duluca/angular-unit-test-helper#readme",
  "devDependencies": {
    "@angular/core": "^7.2.13",
    "@types/jasmine": "^3.4.6",
    "dev-norms": "^1.7.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^4.0.2",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "prettier-tslint": "^0.4.2",
    "rxjs": "^6.4.0",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.1",
    "typescript": "^3.4.3",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  },
  "peerDependencies": {
    "rxjs": ">= 5 <= 9",
    "@angular/core": ">= 4 <= 10",
    "typescript": ">= 2 <= 4",
    "jasmine": ">= 3 <= 4"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module",
      "options": {}
    }
  },
  "dependencies": {
    "camel-to-kebab": "^1.1.0"
  }
}
